#!/usr/bin/env node

var HttpServer = require('../lib/http-server');
var SocketServer = require('../sockets/socket-server');
var ShellBehavior = require('../sockets/shell-behavior');

var httpServer = new HttpServer().start(process.env.PORT || '3000');
var socketServer = new SocketServer().start(httpServer);
var shellBehavior = new ShellBehavior().registerSelf(socketServer);
